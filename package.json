{
  "name": "js-app-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass:build": "sass src/development/core/css/main.scss src/core/css/core.css && sass src/development/custom/css/main.scss src/custom/css/custom.css",
    "sass:watch": "sass --watch src/development/core/css/main.scss src/core/css/core.css",
    "ts:build": "tsc -p .",
    "ts:watch": "tsc -p . -w",
    "lint": "eslint src/ts --ext .ts",
    "format": "prettier -w .",
    "dev": "cross-env NODE_ENV=development/core grunt dev",
    "build": "cross-env NODE_ENV=production grunt build",
    "postcss:build": "postcss src/core/css/core.css --use autoprefixer --no-map -o src/core/css/core.css",
    "prod": "cross-env NODE_ENV=production grunt prod",
    "preview": "cross-env NODE_ENV=production grunt preview",
    "ts:bundle": "esbuild src/development/core/ts/main.ts --bundle --platform=browser --target=es2019 --outfile=src/development/core/js/core-ts.js --format=iife --global-name=App",
    "js:bundle": "esbuild src/development/core/js/core.js --bundle --platform=browser --target=es2019 --outfile=src/development/core/js/core-js.js --format=iife"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.14.1",
    "jquery-ui-dist": "^1.13.3"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.33",
    "@types/jqueryui": "^1.12.24",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.10",
    "eslint": "^8.57.0",
    "grunt": "^1.6.1",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-connect": "^4.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^4.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-exec": "^3.0.0",
    "grunt-postcss": "^0.9.0",
    "grunt-text-replace": "^0.4.0",
    "load-grunt-tasks": "^5.1.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.93.2",
    "typescript": "^5.9.3"
  }
}
